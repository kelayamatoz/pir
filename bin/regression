#!/bin/bash

backend=P14x14
project=pirTest
thread=30

starttime="$(date +'%m_%d_%y_%H_%M_%S')"
log=logs/${backend}_${project}_${starttime}.log

mkdir -p logs/
sbt "; project pir; publishAll"
bin/spatial -t $thread -p $project -b $backend
git log --pretty=format:'%h' -n 1 > $log
bin/log ../gen/$backend | tee -a $log
